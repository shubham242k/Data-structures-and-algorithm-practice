class Tree
{
    // Return a list containing the inorder traversal of the given tree
    ArrayList<Integer> inOrder(Node root)
    {
        Stack<Node> s = new Stack<>();
        Node cur = root;
        // Node prev = null;
        ArrayList<Integer> list = new ArrayList<>();
        while(!s.empty()||cur!=null){
            if(cur!=null){
                s.push(cur);
                
                cur = cur.left;
            }else{
                Node p = s.peek();
                
                cur = p.right;
                list.add(p.data);
            }
        }
        
        return list;
        // Code
    }
    
    
}