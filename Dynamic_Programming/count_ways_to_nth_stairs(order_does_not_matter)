class Solution
{
    public long nthStair(int n)
    {
        // Code here
        int[] d = new int[n+1];
        d[0] = 1;
        d[1] = 1;
        for(int i = 2;i<=n;i++){
                d[i] = d[i-2] + 1;
               
            }
          
        
        
        return d[n] ;
    }
}